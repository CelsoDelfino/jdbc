package application;

import java.io.IOException;
import java.sql.Connection;

import db.DB;
import db.DbException;

public class Program {

	public static void main(String[] args) {
		
		try {
			Connection conn = DB.getConnection();
			DB.closeConnection();			
		}
		catch(IOException e) {
			throw new DbException(e.getMessage());
		}

	}

}
